#!/usr/bin/env php
<?php

require __DIR__.'/../../../autoload.php';

// these must be initialized in the global context
$system_path = null;
$assign_to_config = [];

$app = new eecli\Application();

if ($app->canBeBootstrapped()) {
    require __DIR__.'/../../../eecli/bootstrap/bootstrap-ee2.php';
}

$app->addUserDefinedCommands();

$app->run();
